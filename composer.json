{
	"name": "ondrejmirtes/lunchbot",
	"description": "Bot for posting nearby daily lunch menus to a Slack channel",
	"license": [
		"MIT"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.0",
		"guzzlehttp/guzzle": "~6.1",
		"nette/bootstrap": "~2.3.0",
		"nette/utils": "~2.3.7",
		"symfony/console": "~2.7",
		"symfony/css-selector": "~2.7",
		"symfony/dom-crawler": "~2.7"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "~0.7",
		"phpunit/phpunit": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Lunchbot\\": "src/"
		}
	}
}
